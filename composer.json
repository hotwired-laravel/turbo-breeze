{
    "name": "hotwired-laravel/turbo-breeze",
    "description": "Minimal Laravel authentication scaffolding with Turbo.",
    "keywords": ["laravel", "auth", "hotwired", "turbo"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/hotwired-laravel/turbo-breeze/issues",
        "source": "https://github.com/hotwired-laravel/turbo-breeze"
    },
    "authors": [
        {
            "name": "Tony Messias",
            "email": "tonysm@hey.com"
        }
    ],
    "require": {
        "php": "^8.1.0",
        "illuminate/console": "^10.17",
        "illuminate/filesystem": "^10.17",
        "illuminate/support": "^10.17",
        "illuminate/validation": "^10.17"
    },
    "autoload": {
        "psr-4": {
            "HotwiredLaravel\\TurboBreeze\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "HotwiredLaravel\\TurboBreeze\\BreezeServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "orchestra/testbench": "^8.0",
        "phpstan/phpstan": "^1.10"
    }
}
